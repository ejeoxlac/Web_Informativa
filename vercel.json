{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/Assets/(.*)",
      "dest": "/Src/Assets/$1"
    },
    {
      "src": "/Styles/(.*)",
      "dest": "/Src/Styles/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/Src/js/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 